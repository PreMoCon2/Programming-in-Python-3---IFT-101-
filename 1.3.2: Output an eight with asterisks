# Print the top row of asterisks
print("*****")

# Print the first middle row with asterisk on the first and fifth position, and spaces elsewhere
print("*   *")

# Print the second row of asterisks
print("*****")

# Print the second middle row with asterisk on the first and fifth position, and spaces elsewhere
print("*   *")

# Print the bottom row of asterisks
print("*****")
